source 'https://mirrors.tuna.tsinghua.edu.cn/git/CocoaPods/Specs.git'
platform :ios, '13.0'
use_frameworks!

target 'ZJSwiftBaseProject' do
  #带动画的TabBar
  pod 'RAMAnimatedTabBarController'
  #全屏返回手势 跳转至有 UIScrollView 的控制器时,需要在 UIScrollView 内部完成返回手势,只需如下设置:scrollView.sh_scrollViewPopGestureRecognizerEnable = true
  pod 'SHFullscreenPopGestureSwift'
  pod 'Hue'
end
